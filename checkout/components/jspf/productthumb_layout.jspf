@media only screen and (min-width :120em)
{
    /* content max */
    .l-productthumb .prod-artc
    {min-width: inherit;max-width: inherit; 

        flex-basis: 252px;
    }
}
@media only screen and (max-width: 813px)
{
    /* tablet exclude ipad portrait */
    .l-productthumb .prod-qv__vect
    {
        display: none;
    }
    .l-productthumb .productcard__a--wishlist
    {
        display: none;
    }    
}
@media only screen and (min-width: 701px) and (max-width: 1064px)
{
    /* med */
    .l-productthumb .prod-c2a
    {
        font-size: 14px; width: 68%;
    }
    .l-productthumb .prod-caption
    {
        font-size: 12px;
    }
}
@media only screen and (min-width: 1001px) and (max-width: 1064px)
{
    /* med landscape */
    .l-productthumb .prod-artc
    {min-width: 168px;
        max-width: 184px; 
    }
    /* .l-productthumb .prod-img
    {
        min-width: 168px;
    } */
}
@media only screen and (min-width: 701px) and (max-width: 1000px)
{
    /* med portrait */
    .l-productthumb .prod-artc
    {min-width: 168px;
        max-width: 172px; 
    }
    /* .l-productthumb .prod-img
    {
        min-width: 156px;
    } */
}
@media only screen and (max-width: 700px)
{
    /* small */
    .l-productthumb .productcard__a--wishlist
    {
        display: none;
    }
    /* .l-productthumb .prod-img
    {
        min-width: 150px;
    } */
    /*.l-productthumb .prod-qv__vect { display: none; }*/
    .l-productthumb .prod-c2a
    {
        font-size: 14px; width: 68%;
    }
    .l-productthumb .prod-caption
    {
        font-size: 12px; max-height: 40px;
    }
}
@media only screen and (min-width: 409px) and (max-width: 424px)
{
    /* iphone 6 plus*/
    .l-productthumb .prod-artc
    {min-width: 158px;
        max-width: 190px; 
        margin: 4px;
    }
    /* .l-productthumb .prod-img
    {
        min-width: 174px;
    } */
}
@media only screen and (min-width: 361px) and (max-width: 408px)
{
    /* iphone 6 */
    .l-productthumb .prod-artc
    {min-width: 148px;
        max-width: 170px; 
        margin: 4px;
    }
    /* .l-productthumb .prod-img
    {
        min-width: 154px;
    } */
}
@media only screen and (max-width: 360px)
{
    /* iphone 5*/
    .l-productthumb .prod-artc
    {min-width: 148px;
        max-width: 150px; 
        margin: 2px;
    }
    /* .l-productthumb .prod-img
    {
        min-width: 134px;
    } */
}

@supports (display: grid)
{
    .l-productthumb > section
    {
        display: grid;

        justify-content: center;
        grid-template-columns: repeat(auto-fill, 242px);
    }
    @media only screen and (min-width: 409px) and (max-width: 424px)
    {
        /* iphone 6 plus*/
        .l-productthumb > section
        {
            grid-template-columns: repeat(auto-fill, 176px);
        }
    }
    @media only screen and (min-width: 361px) and (max-width: 408px)
    {
        /* iphone 6 */
        .l-productthumb > section
        {
            grid-template-columns: repeat(auto-fill, 156px);
        }
    }
    @media only screen and (max-width: 360px)
    {
        /* iphone 5*/
        .l-productthumb > section
        {
            grid-template-columns: repeat(auto-fill, 152px);
        }
    }
}


</style>